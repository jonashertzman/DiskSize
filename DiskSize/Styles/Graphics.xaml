<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<DataTemplate x:Key="AnalyzeIcon">
		<Viewbox>
			<Canvas Width="160" Height="160">
				<Path StrokeThickness="0">
					<Path.Fill>
						<LinearGradientBrush StartPoint="0.4,0" EndPoint="0.6,1">
							<GradientStop Offset="0" Color="#FFFFDD7C" />
							<GradientStop Offset="1" Color="#FFFFC834" />
						</LinearGradientBrush>
					</Path.Fill>
					<Path.Data>
						<PathGeometry>
							<PathGeometry.Figures>
								<PathFigure IsClosed="True" StartPoint="0,10">
									<PathFigure.Segments>
										<LineSegment Point="160,10" />
										<LineSegment Point="160,130" />
										<LineSegment Point="0,130" />
									</PathFigure.Segments>
								</PathFigure>
							</PathGeometry.Figures>
						</PathGeometry>
					</Path.Data>
				</Path>
				<Path StrokeThickness="0">
					<Path.Fill>
						<LinearGradientBrush StartPoint="0.4,0" EndPoint="0.6,1">
							<GradientStop Offset="0" Color="#FFE09F00" />
							<GradientStop Offset="1" Color="#FFD79400" />
						</LinearGradientBrush>
					</Path.Fill>
					<Path.Data>
						<PathGeometry>
							<PathGeometry.Figures>
								<PathFigure IsClosed="True" StartPoint="0,0">
									<PathFigure.Segments>
										<LineSegment Point="60,0" />
										<LineSegment Point="80,10" />
										<LineSegment Point="60,30" />
										<LineSegment Point="00,30" />
									</PathFigure.Segments>
								</PathFigure>
							</PathGeometry.Figures>
						</PathGeometry>
					</Path.Data>
				</Path>
				<Path StrokeThickness="16" Fill="Transparent">
					<Path.Stroke>
						<LinearGradientBrush StartPoint="0,0" EndPoint="1,1">
							<GradientStop Offset="0" Color="#FF707070" />
							<GradientStop Offset="1" Color="#FF3C3C3C" />
						</LinearGradientBrush>
					</Path.Stroke>
					<Path.Data>
						<GeometryGroup FillRule="Nonzero">
							<EllipseGeometry Center="75,75" RadiusX="33" RadiusY="33" />
							<LineGeometry StartPoint="100,100" EndPoint="150,150" />
						</GeometryGroup>
					</Path.Data>
				</Path>
			</Canvas>
		</Viewbox>
	</DataTemplate>

	<DataTemplate x:Key="UpIcon">
